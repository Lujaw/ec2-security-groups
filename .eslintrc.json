{
  "env": {
    "commonjs": true,
      "es6": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
    "globals": {
    "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "exports": "always-multiline",
        "functions": "never",
        "imports": "always-multiline",
        "objects": "always-multiline"
      }
    ]
  }
}